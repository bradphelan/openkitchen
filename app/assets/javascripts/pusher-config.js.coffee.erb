<% require 'pusher' %>

Pusher.log = (message) ->
  window.console.log message  if window.console and window.console.log

WEB_SOCKET_DEBUG = true
pusher = new Pusher("<%= Pusher.key %>")
channel = pusher.subscribe("event.11")

# Bind the the create
channel.bind "event_comments", (data) ->
  $.post(data.callback)

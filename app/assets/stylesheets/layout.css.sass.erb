$basefont: 22px
$baseline: 22px

$total-cols: 6                   
$col-width: 4em                    
$gutter-width: 1em                 
$side-gutter-width: 0em

@import 'compass-bootstrap/compass_bootstrap'
@import 'susy/susy'

label
  padding-bottom: 5px

html
  height: 100%
  body
    height: 100%
    background-image: url(<%= asset_path 'fitwaby.png' %>)
    background-repeat: repeat
    background-attachment: scroll

#page
  +container
  .content
    +columns(6)
    margin-right: 0px !important
    margin-left: 0px !important

= form_for(resource, :as => resource_name, :url => user_registration_path(@account)) do |f|
  = devise_error_messages!     
  %p
    = f.label :email
    = f.email_field :email
  %p.clearfix
    = f.submit t('devise.links.sign_up')
  = link_to 'Home', root_url
  %br/ 
  = render :partial => 'devise/links'

